{
    "name": "hennest/laravel-money",
    "description": "Laravel money provider for laravel apps",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "hennest",
            "email": "ernestuwadiegwu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hennest\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hennest\\Money\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "akaunting/laravel-money": "^2.1",
        "guzzlehttp/guzzle": "^7.8",
        "hennest/laravel-math": "0.0.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^8.14",
        "pestphp/pest": "^2.24",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "stan": [
            "./vendor/bin/phpstan analyse"
        ],
        "pest": [
            "./vendor/bin/pest"
        ],
        "pint": [
            "./vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hennest\\Money\\Providers\\MoneyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
